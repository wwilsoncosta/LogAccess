
@{
    ViewBag.Title = "Manutenção de usuários";
}

<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
 
</head>
<body>
    <div id="header" class="topnav navbar-fixed-top">
 
    </div>
    <div id="main" class="container body-content">
        @RenderBody()
    </div>
    <div id="footer" class="row center-block" >
        <hr />
        <footer>
            <p class="text-center">&copy; @DateTime.Now.Year  </p>
        </footer>
    </div>
</body>
</html>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)